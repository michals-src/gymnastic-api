{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": false,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types/2023-07-01"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"paths": {
			"@src/*": ["./src/*"],
			"@db/*": ["./src/db/*"],
			"@routes/*": ["./src/routes/*"]
		}
	}
}
